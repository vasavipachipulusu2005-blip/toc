#include <stdio.h>
#include <string.h>

int main() {
    char s[100];
    printf("Enter string: ");
    scanf("%s", s);

    if (s[0] == '0' && s[strlen(s)-1] == '1')
        printf("String belongs to the CFG (Accepted)\n");
    else
        printf("String does not belong to the CFG (Rejected)\n");

    return 0;
}
