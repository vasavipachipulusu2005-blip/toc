#include <stdio.h>
#include <string.h>

int main() {
    char s[100];
    int i, len, flag = 1;

    printf("Enter string (use e for epsilon): ");
    scanf("%s", s);

    if (strcmp(s, "e") == 0) {
        printf("Accepted\n");
        return 0;
    }

    len = strlen(s);

    for (i = 0; i < len / 2; i++) {
        if (s[i] != s[len - i - 1]) {
            flag = 0;
            break;
        }
    }

    if (flag)
        printf("Accepted\n");
    else
        printf("Rejected\n");

    return 0;
}
